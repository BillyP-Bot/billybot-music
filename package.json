{
  "name": "billybot-music",
  "description": "The one and only BillyP Bot of Boy Town! But just the music playing part.",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . && prettier --check .",
    "format": "eslint --fix --ext .js,.jsx,.ts,.tsx . && prettier --write .",
    "watch": "tsc --watch",
    "start": "node .",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts"
  },
  "author": "Chris Leveille",
  "license": "MIT",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.3.3",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "@distube/soundcloud": "^1.3.3",
    "@distube/spotify": "^1.5.1",
    "@distube/yt-dlp": "^1.1.3",
    "@distube/ytdl-core": "^4.11.10",
    "discord.js": "^14.11.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.11"
  }
}
